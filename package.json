{
  "name": "examples-webpack-caceresjs",
  "version": "1.0.0",
  "description": "Ejemplos para la charla de 'Webpack desde cero' en CaceresJS",
  "main": "index.js",
  "scripts": {
    "dev": "webpack  --mode development",
    "build": "webpack  --mode production",
    "custom": "webpack --mode development examples/custom/src/index.js --output examples/custom/dist/main.js",
    "entry-output": "webpack --config examples/entry-output/webpack.config.js",
    "loader-css-style": "webpack --config examples/loader-css-style/webpack.config.js",
    "plugin-css-extract": "webpack --config examples/plugin-css-extract/webpack.config.js",
    "multi-entry-points": "webpack --config examples/multi-entry-points/webpack.config.js",
    "webpack-dev-server": "webpack-dev-server --config examples/webpack-dev-server/webpack.config.js",
    "loader-babel": "webpack --config examples/loader-babel/webpack.config.js",
    "loader-url-images": "webpack --config examples/loader-url-images/webpack.config.js",
    "loader-url-fonts": "webpack --config examples/loader-url-fonts/webpack.config.js",
    "loader-files": "webpack --config examples/loader-files/webpack.config.js",
    "loader-json": "webpack --config examples/loader-json/webpack.config.js",
    "react": "webpack --config examples/react/webpack.config.js",
    "loader-sass": "webpack --config examples/loader-sass/webpack.config.js",
    "commons-chunk": "webpack --config examples/commons-chunk/webpack.config.js",
    "dynamic-import": "webpack --config examples/dynamic-import/webpack.config.js"
  },
  "keywords": [
    "webpack"
  ],
  "author": "@javiernuber",
  "license": "MIT",
  "devDependencies": {
    "@babel/core": "^7.1.2",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-transform-runtime": "^7.1.0",
    "@babel/preset-env": "^7.1.0",
    "@babel/preset-react": "^7.0.0",
    "@babel/runtime": "^7.1.2",
    "babel-loader": "^8.0.4",
    "css-loader": "^1.0.0",
    "file-loader": "^2.0.0",
    "mini-css-extract-plugin": "^0.4.3",
    "node-sass": "^4.9.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.0",
    "url-loader": "^1.1.1",
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1",
    "webpack-dev-server": "^3.1.9"
  },
  "dependencies": {
    "react": "^16.5.2",
    "react-dom": "^16.5.2"
  }
}
