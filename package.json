{
  "name": "examples-webpack-caceresjs",
  "version": "1.0.0",
  "description": "Ejemplos para la charla de 'Webpack desde cero' en CaceresJS",
  "main": "index.js",
  "scripts": {
    "dev": "webpack  --mode development",
    "build": "webpack  --mode production",
    "p1:dev": "webpack --mode development ./p1/src/index.js --output ./p1/dist/main.js",
    "p1:build": "webpack --mode production ./p1/src/index.js --output ./p1/dist/main.js",
    "p2:build": "webpack --config p2/prod.config.js",
    "p2:dev": "webpack --config p2/dev.config.js" 
  },
  "keywords": [
    "webpack"
  ],
  "author": "@javiernuber",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1"
  }
}
