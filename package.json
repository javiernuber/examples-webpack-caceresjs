{
  "name": "examples-webpack-caceresjs",
  "version": "1.0.0",
  "description": "Ejemplos para la charla de 'Webpack desde cero' en CaceresJS",
  "main": "index.js",
  "scripts": {
    "dev": "webpack  --mode development",
    "build": "webpack  --mode production",
    "test:dev": "webpack --mode development examples/test/src/index.js --output examples/test/dist/main.js",
    "test:build": "webpack --mode production examples/test/src/index.js --output examples/test/dist/main.js",
    "entry-output:build": "webpack --config examples/entry-output/prod.config.js",
    "entry-output:dev": "webpack --config examples/entry-output/dev.config.js" 
  },
  "keywords": [
    "webpack"
  ],
  "author": "@javiernuber",
  "license": "MIT",
  "devDependencies": {
    "webpack": "^4.19.1",
    "webpack-cli": "^3.1.1"
  }
}
